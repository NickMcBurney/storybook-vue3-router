{
  "name": "storybook-vue3-router",
  "version": "6.0.0",
  "description": "A Storybook decorator that allows you to build stories for your routing-aware components.",
  "keywords": [
    "storybook-addons",
    "vue",
    "vue-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NickMcBurney/storybook-vue3-router"
  },
  "author": "Nick McBurney",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "pnpm clean",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "storybook": "storybook dev -p 6006",
    "dev": "concurrently \"pnpm storybook\" \"pnpm build:watch\"",
    "build-storybook": "storybook build",
    "prepublish": "pnpm build",
    "test": "npx playwright test"
  },
  "dependencies": {
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/vue3-vite": "^9.0.18",
    "@types/node": "^18.19.120",
    "@vitejs/plugin-vue": "^4.6.2",
    "concurrently": "^6.5.1",
    "esbuild": "^0.17.19",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.29.5",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "storybook": "^9.0.18",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vue": "^3.5.18",
    "zx": "^1.15.2"
  },
  "peerDependencies": {
    "@storybook/vue3": "9.x",
    "vue": "3.x",
    "vue-router": "4.x"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Vue 3 Router",
    "supportedFrameworks": [
      "vue"
    ],
    "icon": "https://github.com/NickMcBurney/storybook-vue3-router/blob/main/vue-logo.png?raw=true"
  }
}
